{
    "name":  "fullpos-backend",
    "version":  "1.0.0",
    "private":  true,
    "type":  "commonjs",
    "scripts":  {
                    "dev":  "ts-node-dev --respawn --transpile-only src/server.ts",
                    "build":  "prisma generate \u0026\u0026 tsc -p tsconfig.json",
                    "start":  "node dist/server.js",
                    "migrate":  "prisma migrate deploy",
                    "start:migrate":  "npm run migrate \u0026\u0026 node dist/server.js",
                    "migrate:dev":  "prisma migrate dev",
                    "generate":  "prisma generate",
                    "seed":  "ts-node prisma/seed.ts",
                    "token:integration:create":  "ts-node --files tools/create_integration_token.ts",
                    "test:integration:products":  "ts-node --files tools/test_integration_products_tenant_isolation.ts",
                    "lint":  "echo \"No linter configured\""
                },
    "prisma":  {
                   "seed":  "ts-node prisma/seed.ts"
               },
    "dependencies":  {
                         "@prisma/client":  "^5.11.0",
                         "bcryptjs":  "^2.4.3",
                         "cors":  "^2.8.5",
                         "date-fns":  "^3.6.0",
                         "date-fns-tz":  "^3.2.0",
                         "dotenv":  "^16.4.5",
                         "express":  "^4.19.2",
                         "helmet":  "^7.1.0",
                         "jsonwebtoken":  "^9.0.2",
                         "morgan":  "^1.10.0",
                         "multer":  "^1.4.5-lts.1",
                         "sharp":  "^0.33.5",
                         "zod":  "^3.23.8"
                     },
    "devDependencies":  {
                            "@types/bcryptjs":  "^2.4.6",
                            "@types/cors":  "^2.8.17",
                            "@types/express":  "^4.17.21",
                            "@types/jsonwebtoken":  "^9.0.6",
                            "@types/morgan":  "^1.9.7",
                            "@types/multer":  "^1.4.12",
                            "@types/node":  "^22.7.5",
                            "@types/supertest":  "^6.0.2",
                            "prisma":  "^5.11.0",
                            "supertest":  "^7.0.0",
                            "ts-node":  "^10.9.2",
                            "ts-node-dev":  "^2.0.0",
                            "typescript":  "^5.6.3"
                        }
}
